using System;

namespace Array_1
{
    class Program
    {
        static void Main(string[] args)
        {
            int[,] map = new int[2, 3];

            // [ , , ]
            // [ , , , , , ]
            // [ , , , ]

            // 가변배열
            int[][] a = new int[2][];
            a[0] = new int[3];
            a[1] = new int[6];
            a[2] = new int[2];

            a[0][0] = 1;
        }
    }
}
